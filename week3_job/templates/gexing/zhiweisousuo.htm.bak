</body>
<script>
    function IMG1_onclick() 
    {
        var ctlzhiwei_name="name="+escape(document.getElementById("ctlzhiwei_name").value);
        var ctlqw_guozuohy="&hy="+document.getElementById("ctlqw_guozuohy").value;
        var txtsuoshuqy="&dq="+document.getElementById("txtsuoshuqy").value;
        var ctlriqi="&rq="+document.getElementById("ctlriqi").value;

         
        var ctlqiye_name="&qn="+escape(document.getElementById("ctlqiye_name").value);
        var ctlxingzhi="&xz="+escape(document.getElementById("ctlxingzhi").value);
        var ctlxingbieyq="&xb="+escape(document.getElementById("ctlxingbieyq").value);
    
        var page="&pg=1";
        var url="zhiweisousuo.aspx?"+ctlzhiwei_name+ctlqw_guozuohy+txtsuoshuqy+ctlriqi+ctlqiye_name+ctlxingzhi+ctlxingbieyq+page;
       
        //
        window.location.href=url;
    }
</script>

</html>
