                    <tr>
                        <td style="height:3px;">
                        
                        </td>
                     </tr>
                    </table>
                        
                       
                        <div class="xinwenlanDiv" style="margin-top: 4px; width: 780px;margin-top:8px;font-size:13px;
                            ">
                            <div class="xinwenlanDiv_top">
                                <div class="xinwenlanDiv_top_tou2">
                                </div>
                                <a class="kuangBiaotiWhite" href="#" style="left: -53px; float: left; position: relative">
                                    招聘职位 </a>
                            </div>
                          
                            <div class="liebiaoBiaoti" style="width:780px;height:24px;">
                                   <div style="width:74px;">发布日期</div>
                                   
                                   <div style="width:235px;">招聘职位</div>
                                   <div style="width:206px;">
                                       单位名称</div>
